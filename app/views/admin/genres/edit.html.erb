<div class="container">
  <span class="bg-light mx-5 h4">　ジャンル編集　</span>
  <div class="row mt-5">
    <%= form_with model: [:admin, @genre] do |f| %>
      <label for="genre_name">ジャンル名</label>
      <%= f.text_field :name , class:"mx-5" %>
      <%= f.submit '変更を保存', class:"btn btn-lg btn-success" %>
    <% end %>
  </div>
</div>