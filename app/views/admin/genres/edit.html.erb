<div class="container">
  <h5><span class="bg-light">　ジャンル編集　</span></h5>
  <div class="row">
    <%= form_with model: [:admin, @genre] do |f| %>
      <label for="genre_name">ジャンル名</label>
      <%= f.text_field :name %>
      <%= f.submit '変更を保存', class:"btn btn-success" %>
    <% end %>
  </div>
</div>